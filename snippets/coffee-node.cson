'.source.coffee':
  'module-exports':
    'prefix': 'me'
    'body':
      """
      module.exports = $0
      """

  'require':
    'prefix': 'rrr'
    'body':
      """
      require('${1}')$0
      """


  'require lodash':
    'prefix': 'rrl'
    'body':
      """
      _ = require('lodash')$0
      """
