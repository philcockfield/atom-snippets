'.source.coffee':

  'require expect':
    'prefix': 're'
    'body':
      """
      expect = require('chai').expect
      """



  'expect: toEqual':
    'prefix': 'exe'
    'body':
      """
      expect(${1:value}).to.equal ${2:result} $0
      """



  'expect: NOT toEqual':
    'prefix': 'ene'
    'body':
      """
      expect(${1:value}).not.to.equal ${2:result} $0
      """
